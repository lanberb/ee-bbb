{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "ee-bbb",
	"main": "src/server.tsx",
	"compatibility_date": "2025-09-27",
	"compatibility_flags": [
		"nodejs_compat",
		"global_fetch_strictly_public"
	],
	"assets": {
		"directory": "./dist",
    "binding": "ASSETS"
	},
	"rules": [{
		"type": "Data",
		"globs": ["**/*.png"],
		"fallthrough": true
	}],
	"observability": {
		"enabled": true
	}
}